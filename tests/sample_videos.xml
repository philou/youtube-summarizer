   <media:title>057 - Pesticides and low brain choline</media:title>
   <media:content url="https://www.youtube.com/v/43BHyr0-BNA?version=3" type="application/x-shockwave-flash" width="640" height="390"/>
   <media:thumbnail url="https://i1.ytimg.com/vi/43BHyr0-BNA/hqdefault.jpg" width="480" height="360"/>
